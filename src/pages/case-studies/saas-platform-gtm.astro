<script>
  // Add any interactive functionality here
  document.addEventListener('DOMContentLoaded', function() {
    // Smooth scrolling for anchor links
    document.querySelectorAll('a[href^="#"]').forEach(anchor => {
      anchor.addEventListener('click', function (e) {
        e.preventDefault();
        const target = document.querySelector(this.getAttribute('href'));
        if (target) {
          target.scrollIntoView({
            behavior: 'smooth',
            block: 'start'
          });
        }
      });
    });

    // Chart animation functionality
    function animateChart(chartElement) {
      // Animate SVG lines
      const lines = chartElement.querySelectorAll('polyline');
      lines.forEach(line => {
        const length = line.getTotalLength();
        line.style.strokeDasharray = length;
        line.style.strokeDashoffset = length;
        line.style.animation = 'drawLine 2s ease-in-out forwards';
      });

      // Animate circles
      const circles = chartElement.querySelectorAll('circle');
      circles.forEach((circle, index) => {
        circle.style.opacity = '0';
        circle.style.transform = 'scale(0)';
        setTimeout(() => {
          circle.style.animation = 'popIn 0.5s ease-out forwards';
        }, 1500 + (index * 100));
      });

      // Animate bars
      const bars = chartElement.querySelectorAll('rect');
      bars.forEach((bar, index) => {
        const height = bar.getAttribute('height');
        const y = bar.getAttribute('y');

        // Skip background/grid rectangles and only animate data bars
        if (height && height !== '100%' && !bar.getAttribute('fill')?.includes('url(#grid)') && parseFloat(height) > 0) {
          const originalHeight = parseFloat(height);
          const originalY = parseFloat(y);

          // Start with zero height and position at bottom
          bar.setAttribute('height', '0');
          bar.setAttribute('y', originalY + originalHeight);
          bar.style.opacity = '0.3';

          setTimeout(() => {
            bar.style.transition = 'all 1.2s cubic-bezier(0.4, 0, 0.2, 1)';
            bar.setAttribute('height', originalHeight.toString());
            bar.setAttribute('y', originalY.toString());
            bar.style.opacity = '0.8';
          }, 800 + (index * 150));
        }
      });

      // Animate progress bars
      const progressBars = chartElement.querySelectorAll('[style*="width:"]');
      progressBars.forEach((bar, index) => {
        const originalWidth = bar.style.width;
        bar.style.width = '0%';
        setTimeout(() => {
          bar.style.transition = 'width 1.5s ease-out';
          bar.style.width = originalWidth;
        }, index * 300);
      });
    }

    // Add fade-in animation for sections as they come into view
    const observerOptions = {
      threshold: 0.3,
      rootMargin: '0px 0px -50px 0px'
    };

    const observer = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          entry.target.style.opacity = '1';
          entry.target.style.transform = 'translateY(0)';

          // Check if this section contains charts and animate them
          const charts = entry.target.querySelectorAll('svg, .bg-glass-gray-50, .bg-dark-glass-gray-50');
          charts.forEach(chart => {
            if (!chart.dataset.animated) {
              chart.dataset.animated = 'true';
              setTimeout(() => animateChart(chart), 500);
            }
          });
        }
      });
    }, observerOptions);

    // Observe all sections
    document.querySelectorAll('section').forEach(section => {
      section.style.opacity = '0';
      section.style.transform = 'translateY(20px)';
      section.style.transition = 'opacity 0.6s ease, transform 0.6s ease';
      observer.observe(section);
    });

    // Add CSS animations
    const style = document.createElement('style');
    style.textContent = `
      @keyframes drawLine {
        to {
          stroke-dashoffset: 0;
        }
      }

      @keyframes popIn {
        to {
          opacity: 1;
          transform: scale(1);
        }
      }

      .chart-animate {
        animation: fadeInUp 0.8s ease-out forwards;
      }

      @keyframes fadeInUp {
        from {
          opacity: 0;
          transform: translateY(30px);
        }
        to {
          opacity: 1;
          transform: translateY(0);
        }
      }
    `;
    document.head.appendChild(style);
  });
</script>
