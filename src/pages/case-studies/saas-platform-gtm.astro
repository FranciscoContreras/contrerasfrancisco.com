---
import Layout from '../../layouts/Layout.astro';
import Navigation from '../../components/Navigation.astro';
import Footer from '../../components/Footer.astro';
---

<Layout title="SaaS Platform Go-to-Market Case Study - Francisco Contreras">
  <Navigation />

  <!-- Hero Section -->
  <section class="bg-glass-bg dark:bg-dark-glass-bg pt-32 pb-8 transition-colors duration-300">
    <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
      <!-- Breadcrumb -->
      <nav class="mb-8">
        <div class="flex text-sm text-glass-gray-500 dark:text-dark-glass-gray-500">
          <a href="/" class="hover:text-glass-primary dark:hover:text-dark-glass-primary transition-colors">Home</a>
          <span class="mx-2">/</span>
          <a href="/#case-studies" class="hover:text-glass-primary dark:hover:text-dark-glass-primary transition-colors">Case Studies</a>
          <span class="mx-2">/</span>
          <span class="text-glass-gray-700 dark:text-dark-glass-gray-700">SaaS Platform Go-to-Market</span>
        </div>
      </nav>

      <!-- Hero Content -->
      <div class="grid lg:grid-cols-2 gap-12 items-center">
        <div>
          <div class="inline-flex items-center px-3 py-1 rounded-full bg-glass-primary/10 dark:bg-dark-glass-primary/10 text-glass-primary dark:text-dark-glass-primary text-sm font-medium mb-6">
            <svg class="w-4 h-4 mr-2" fill="currentColor" viewBox="0 0 20 20">
              <path d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"/>
            </svg>
            Product Launch Case Study
          </div>

          <h1 class="text-3xl md:text-4xl lg:text-5xl font-serif italic font-bold text-glass-gray-900 dark:text-dark-glass-gray-900 mb-4 leading-tight">
            AI-Powered SaaS Platform
            <span class="text-glass-primary dark:text-dark-glass-primary">Go-to-Market</span>
          </h1>

          <p class="text-lg font-sans text-glass-gray-600 dark:text-dark-glass-gray-600 mb-6 leading-relaxed max-w-2xl">
            How we launched an AI analytics platform in a competitive market, achieving 300% user growth and $2.5M ARR within 6 months through strategic positioning and multi-channel execution.
          </p>

          <!-- Key Metrics Preview -->
          <div class="grid grid-cols-3 gap-3 mb-6">
            <div class="text-center p-3 bg-glass-white/60 dark:bg-dark-glass-white/60 backdrop-blur-lg rounded-glass border border-glass-gray-200/20 dark:border-dark-glass-gray-200/20">
              <div class="text-xl font-sans font-bold text-glass-primary dark:text-dark-glass-primary">+300%</div>
              <div class="text-xs text-glass-gray-600 dark:text-dark-glass-gray-600">User Growth</div>
            </div>
            <div class="text-center p-3 bg-glass-white/60 dark:bg-dark-glass-white/60 backdrop-blur-lg rounded-glass border border-glass-gray-200/20 dark:border-dark-glass-gray-200/20">
              <div class="text-xl font-sans font-bold text-glass-primary dark:text-dark-glass-primary">$2.5M</div>
              <div class="text-xs text-glass-gray-600 dark:text-dark-glass-gray-600">ARR</div>
            </div>
            <div class="text-center p-3 bg-glass-white/60 dark:bg-dark-glass-white/60 backdrop-blur-lg rounded-glass border border-glass-gray-200/20 dark:border-dark-glass-gray-200/20">
              <div class="text-xl font-sans font-bold text-glass-primary dark:text-dark-glass-primary">85%</div>
              <div class="text-xs text-glass-gray-600 dark:text-dark-glass-gray-600">Conversion</div>
            </div>
          </div>

          <!-- CTA -->
          <div class="flex flex-col sm:flex-row gap-4">
            <a href="#detailed-analysis" class="group relative bg-glass-primary dark:bg-dark-glass-primary text-glass-white dark:text-dark-glass-white px-6 py-3 rounded-glass text-sm font-medium transition-all duration-500 shadow-glass focus:outline-none focus:ring-2 focus:ring-glass-primary/20 dark:focus:ring-dark-glass-primary/20 overflow-hidden hover:shadow-xl hover:scale-105 hover:bg-white hover:text-glass-primary">
              <span class="relative z-10 transition-colors duration-500">View Full Analysis</span>
              <div class="absolute inset-0 bg-white opacity-0 group-hover:opacity-100 transition-opacity duration-500"></div>
            </a>
            <a href="#methodology" class="border border-glass-gray-300 dark:border-dark-glass-gray-300 text-glass-gray-700 dark:text-dark-glass-gray-700 px-6 py-3 rounded-glass text-sm font-medium hover:bg-glass-gray-50 dark:hover:bg-dark-glass-gray-50 transition-all duration-300 text-center">
              See Methodology
            </a>
          </div>
        </div>

        <!-- Hero Visual -->
        <div class="relative">
          <div class="bg-glass-white/60 dark:bg-dark-glass-white/60 backdrop-blur-lg rounded-glass p-8 border border-glass-gray-200/20 dark:border-dark-glass-gray-200/20 shadow-soft hover:shadow-glass transition-all duration-300">
            <!-- Mock Dashboard -->
            <div class="space-y-4">
              <div class="flex items-center justify-between">
                <h3 class="font-semibold text-glass-gray-800 dark:text-dark-glass-gray-800">Growth Dashboard</h3>
                <div class="flex space-x-2">
                  <div class="w-3 h-3 bg-red-400 rounded-full"></div>
                  <div class="w-3 h-3 bg-yellow-400 rounded-full"></div>
                  <div class="w-3 h-3 bg-green-400 rounded-full"></div>
                </div>
              </div>

              <!-- Mock Chart -->
              <div class="bg-glass-gray-50 dark:bg-dark-glass-gray-50 rounded-lg p-4 h-32 relative overflow-hidden">
                <svg class="w-full h-full" viewBox="0 0 300 100">
                  <polyline
                    fill="none"
                    stroke="currentColor"
                    stroke-width="2"
                    class="text-glass-primary dark:text-dark-glass-primary"
                    points="10,80 50,70 90,45 130,40 170,25 210,20 250,15 290,10"
                  />
                  <circle cx="290" cy="10" r="3" fill="currentColor" class="text-glass-primary dark:text-dark-glass-primary"/>
                </svg>
                <div class="absolute top-2 left-2 text-xs text-glass-gray-500 dark:text-dark-glass-gray-500">User Growth Trajectory</div>
              </div>

              <!-- Mock Metrics -->
              <div class="grid grid-cols-2 gap-3">
                <div class="bg-glass-gray-50 dark:bg-dark-glass-gray-50 rounded p-3">
                  <div class="text-sm text-glass-gray-500 dark:text-dark-glass-gray-500">Monthly Users</div>
                  <div class="text-lg font-bold text-glass-gray-800 dark:text-dark-glass-gray-800">47.2K</div>
                </div>
                <div class="bg-glass-gray-50 dark:bg-dark-glass-gray-50 rounded p-3">
                  <div class="text-sm text-glass-gray-500 dark:text-dark-glass-gray-500">Revenue</div>
                  <div class="text-lg font-bold text-glass-gray-800 dark:text-dark-glass-gray-800">$208K</div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Challenge Section -->
  <section id="challenge" class="pt-4 pb-16 bg-glass-white dark:bg-dark-glass-white transition-colors duration-300">
    <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="grid lg:grid-cols-2 gap-12 items-center">
        <div>
          <h2 class="text-2xl md:text-3xl font-serif italic font-bold text-glass-gray-900 dark:text-dark-glass-gray-900 mb-4 leading-tight">
            The Challenge
          </h2>
          <p class="text-base text-glass-gray-600 dark:text-dark-glass-gray-600 mb-6 leading-relaxed">
            Launching an AI-powered analytics platform in an oversaturated market with established competitors and limited brand awareness. The challenge was to differentiate our offering and build trust in a space where AI credibility was crucial.
          </p>

          <!-- Challenge Points -->
          <div class="space-y-4">
            <div class="flex items-start space-x-3">
              <div class="w-6 h-6 bg-red-100 dark:bg-red-900/30 rounded-full flex items-center justify-center mt-1">
                <svg class="w-3 h-3 text-red-600 dark:text-red-400" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zM8.707 7.293a1 1 0 00-1.414 1.414L8.586 10l-1.293 1.293a1 1 0 101.414 1.414L10 11.414l1.293 1.293a1 1 0 001.414-1.414L11.414 10l1.293-1.293a1 1 0 00-1.414-1.414L10 8.586 8.707 7.293z" clip-rule="evenodd"/>
                </svg>
              </div>
              <div>
                <h4 class="font-semibold text-glass-gray-800 dark:text-dark-glass-gray-800">Market Saturation</h4>
                <p class="text-glass-gray-600 dark:text-dark-glass-gray-600">Over 200 analytics platforms competing for attention in the market</p>
              </div>
            </div>

            <div class="flex items-start space-x-3">
              <div class="w-6 h-6 bg-red-100 dark:bg-red-900/30 rounded-full flex items-center justify-center mt-1">
                <svg class="w-3 h-3 text-red-600 dark:text-red-400" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zM8.707 7.293a1 1 0 00-1.414 1.414L8.586 10l-1.293 1.293a1 1 0 101.414 1.414L10 11.414l1.293 1.293a1 1 0 001.414-1.414L11.414 10l1.293-1.293a1 1 0 00-1.414-1.414L10 8.586 8.707 7.293z" clip-rule="evenodd"/>
                </svg>
              </div>
              <div>
                <h4 class="font-semibold text-glass-gray-800 dark:text-dark-glass-gray-800">Zero Brand Recognition</h4>
                <p class="text-glass-gray-600 dark:text-dark-glass-gray-600">New company with no established credibility or market presence</p>
              </div>
            </div>

            <div class="flex items-start space-x-3">
              <div class="w-6 h-6 bg-red-100 dark:bg-red-900/30 rounded-full flex items-center justify-center mt-1">
                <svg class="w-3 h-3 text-red-600 dark:text-red-400" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zM8.707 7.293a1 1 0 00-1.414 1.414L8.586 10l-1.293 1.293a1 1 0 101.414 1.414L10 11.414l1.293 1.293a1 1 0 001.414-1.414L11.414 10l1.293-1.293a1 1 0 00-1.414-1.414L10 8.586 8.707 7.293z" clip-rule="evenodd"/>
                </svg>
              </div>
              <div>
                <h4 class="font-semibold text-glass-gray-800 dark:text-dark-glass-gray-800">AI Skepticism</h4>
                <p class="text-glass-gray-600 dark:text-dark-glass-gray-600">Target audience skeptical of AI capabilities and ROI claims</p>
              </div>
            </div>

            <div class="flex items-start space-x-3">
              <div class="w-6 h-6 bg-red-100 dark:bg-red-900/30 rounded-full flex items-center justify-center mt-1">
                <svg class="w-3 h-3 text-red-600 dark:text-red-400" fill="currentColor" viewBox="0 0 20 20">
                  <path fill-rule="evenodd" d="M10 18a8 8 0 100-16 8 8 0 000 16zM8.707 7.293a1 1 0 00-1.414 1.414L8.586 10l-1.293 1.293a1 1 0 101.414 1.414L10 11.414l1.293 1.293a1 1 0 001.414-1.414L11.414 10l1.293-1.293a1 1 0 00-1.414-1.414L10 8.586 8.707 7.293z" clip-rule="evenodd"/>
                </svg>
              </div>
              <div>
                <h4 class="font-semibold text-glass-gray-800 dark:text-dark-glass-gray-800">Limited Budget</h4>
                <p class="text-glass-gray-600 dark:text-dark-glass-gray-600">$150K marketing budget competing against enterprise players</p>
              </div>
            </div>
          </div>
        </div>

        <!-- Market Landscape Visual -->
        <div class="bg-glass-white/60 dark:bg-dark-glass-white/60 backdrop-blur-lg rounded-glass p-8 border border-glass-gray-200/20 dark:border-dark-glass-gray-200/20 shadow-soft hover:shadow-glass transition-all duration-300">
          <h4 class="font-sans font-semibold text-glass-gray-800 dark:text-dark-glass-gray-800 mb-6">Competitive Landscape</h4>

          <!-- Market Map -->
          <div class="relative h-80 bg-glass-gray-50 dark:bg-dark-glass-gray-50 rounded-lg overflow-hidden">
            <svg class="w-full h-full" viewBox="0 0 400 300" preserveAspectRatio="xMidYMid meet">
              <!-- Grid background -->
              <defs>
                <pattern id="market-grid" width="40" height="30" patternUnits="userSpaceOnUse">
                  <path d="M 40 0 L 0 0 0 30" fill="none" stroke="currentColor" stroke-width="0.5" class="text-glass-gray-300 dark:text-dark-glass-gray-300" opacity="0.3"/>
                </pattern>
              </defs>
              <rect width="100%" height="100%" fill="url(#market-grid)" />

              <!-- Axes -->
              <line x1="40" y1="260" x2="360" y2="260" stroke="currentColor" stroke-width="2" class="text-glass-gray-400 dark:text-dark-glass-gray-400"/>
              <line x1="40" y1="260" x2="40" y2="40" stroke="currentColor" stroke-width="2" class="text-glass-gray-400 dark:text-dark-glass-gray-400"/>

              <!-- Axis Labels -->
              <text x="360" y="280" text-anchor="end" class="text-xs font-sans fill-current text-glass-gray-600 dark:text-dark-glass-gray-600">Market Share →</text>
              <text x="20" y="45" text-anchor="middle" class="text-xs font-sans fill-current text-glass-gray-600 dark:text-dark-glass-gray-600" transform="rotate(-90, 20, 45)">Innovation →</text>

              <!-- Quadrant Labels -->
              <text x="70" y="70" class="text-xs font-sans fill-current text-glass-gray-500 dark:text-dark-glass-gray-500">High Innovation</text>
              <text x="70" y="85" class="text-xs font-sans fill-current text-glass-gray-500 dark:text-dark-glass-gray-500">Low Share</text>

              <text x="320" y="70" text-anchor="end" class="text-xs font-sans fill-current text-glass-gray-500 dark:text-dark-glass-gray-500">Market Leaders</text>
              <text x="320" y="85" text-anchor="end" class="text-xs font-sans fill-current text-glass-gray-500 dark:text-dark-glass-gray-500">High Share</text>

              <!-- Our Platform (High Innovation, Low Share) -->
              <circle cx="80" cy="80" r="8" fill="currentColor" class="text-glass-primary dark:text-dark-glass-primary" stroke="white" stroke-width="2"/>
              <text x="80" y="105" text-anchor="middle" class="text-xs font-sans font-bold fill-current text-glass-primary dark:text-dark-glass-primary">Our Platform</text>

              <!-- Enterprise Leaders (High Share, High Innovation) -->
              <circle cx="300" cy="70" r="7" fill="currentColor" class="text-glass-primary dark:text-dark-glass-primary" opacity="0.8" stroke="white" stroke-width="2"/>
              <text x="300" y="95" text-anchor="middle" class="text-xs font-sans font-medium fill-current text-glass-gray-700 dark:text-dark-glass-gray-700">Tableau</text>

              <circle cx="280" cy="90" r="6" fill="currentColor" class="text-glass-primary dark:text-dark-glass-primary" opacity="0.7" stroke="white" stroke-width="2"/>
              <text x="280" y="115" text-anchor="middle" class="text-xs font-sans font-medium fill-current text-glass-gray-700 dark:text-dark-glass-gray-700">PowerBI</text>

              <!-- Mid-market Players -->
              <circle cx="220" cy="130" r="6" fill="currentColor" class="text-glass-primary dark:text-dark-glass-primary" opacity="0.6" stroke="white" stroke-width="2"/>
              <text x="220" y="155" text-anchor="middle" class="text-xs font-sans font-medium fill-current text-glass-gray-700 dark:text-dark-glass-gray-700">Looker</text>

              <circle cx="180" cy="180" r="5" fill="currentColor" class="text-glass-primary dark:text-dark-glass-primary" opacity="0.5" stroke="white" stroke-width="2"/>
              <text x="180" y="200" text-anchor="middle" class="text-xs font-sans font-medium fill-current text-glass-gray-700 dark:text-dark-glass-gray-700">Sisense</text>
            </svg>

            <!-- Legend -->
            <div class="absolute bottom-4 right-4 bg-white/95 dark:bg-dark-glass-white/95 rounded-lg p-3 text-xs font-sans backdrop-blur-sm border border-glass-gray-200/20">
              <div class="flex items-center space-x-2 mb-2">
                <div class="w-3 h-3 bg-glass-primary dark:bg-dark-glass-primary rounded-full opacity-80"></div>
                <span class="text-glass-gray-700 dark:text-dark-glass-gray-700">Enterprise Leaders</span>
              </div>
              <div class="flex items-center space-x-2 mb-2">
                <div class="w-3 h-3 bg-glass-primary dark:bg-dark-glass-primary rounded-full opacity-60"></div>
                <span class="text-glass-gray-700 dark:text-dark-glass-gray-700">Mid-market Players</span>
              </div>
              <div class="flex items-center space-x-2">
                <div class="w-3 h-3 bg-glass-primary dark:bg-dark-glass-primary rounded-full"></div>
                <span class="text-glass-primary dark:text-dark-glass-primary font-medium">Our Platform</span>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </section>

  <!-- Strategy Section -->
  <section id="methodology" class="py-12 bg-glass-bg dark:bg-dark-glass-bg transition-colors duration-300">
    <div class="max-w-6xl mx-auto px-4 sm:px-6 lg:px-8">
      <div class="text-center mb-16">
        <h2 class="text-2xl md:text-3xl font-serif italic font-bold text-glass-gray-900 dark:text-dark-glass-gray-900 mb-4 leading-tight">
          Strategic Approach
        </h2>
        <p class="text-lg text-glass-gray-600 dark:text-dark-glass-gray-600 max-w-3xl mx-auto">
          We developed a data-driven positioning strategy targeting mid-market companies with clear ROI messaging, focusing on tangible value demonstration over feature competition.
        </p>
      </div>

      <!-- Strategy Framework -->
      <div class="grid md:grid-cols-3 gap-8 mb-16">
        <div class="group bg-glass-white/60 dark:bg-dark-glass-white/60 backdrop-blur-lg rounded-glass p-8 border border-glass-gray-200/20 dark:border-dark-glass-gray-200/20 shadow-soft hover:shadow-glass text-center transition-all duration-300">
          <div class="w-12 h-12 bg-glass-primary/10 dark:bg-dark-glass-primary/10 rounded-full flex items-center justify-center mx-auto mb-6">
            <svg class="w-6 h-6 text-glass-primary dark:text-dark-glass-primary" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"/>
            </svg>
          </div>
          <h3 class="text-lg font-sans font-semibold text-glass-gray-900 dark:text-dark-glass-gray-900 mb-3">Market Analysis</h3>
          <p class="text-glass-gray-600 dark:text-dark-glass-gray-600 font-sans">Comprehensive analysis of 200+ competitors and 500+ target companies to identify positioning gaps</p>
        </div>

        <div class="group bg-glass-white/60 dark:bg-dark-glass-white/60 backdrop-blur-lg rounded-glass p-8 border border-glass-gray-200/20 dark:border-dark-glass-gray-200/20 shadow-soft hover:shadow-glass text-center transition-all duration-300">
          <div class="w-12 h-12 bg-glass-primary/10 dark:bg-dark-glass-primary/10 rounded-full flex items-center justify-center mx-auto mb-6">
            <svg class="w-6 h-6 text-glass-primary dark:text-dark-glass-primary" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 5.882V19.24a1.76 1.76 0 01-3.417.592l-2.147-6.15M18 13a3 3 0 100-6M5.436 13.683A4.001 4.001 0 017 6h1.832c4.1 0 7.625-1.234 9.168-3v14c-1.543-1.766-5.067-3-9.168-3H7a3.988 3.988 0 01-1.564-.317z"/>
            </svg>
          </div>
          <h3 class="text-lg font-sans font-semibold text-glass-gray-900 dark:text-dark-glass-gray-900 mb-3">Message Development</h3>
          <p class="text-glass-gray-600 dark:text-dark-glass-gray-600 font-sans">ROI-focused messaging framework addressing specific mid-market pain points and decision criteria</p>
        </div>

        <div class="group bg-glass-white/60 dark:bg-dark-glass-white/60 backdrop-blur-lg rounded-glass p-8 border border-glass-gray-200/20 dark:border-dark-glass-gray-200/20 shadow-soft hover:shadow-glass text-center transition-all duration-300">
          <div class="w-12 h-12 bg-glass-primary/10 dark:bg-dark-glass-primary/10 rounded-full flex items-center justify-center mx-auto mb-6">
            <svg class="w-6 h-6 text-glass-primary dark:text-dark-glass-primary" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M13 10V3L4 14h7v7l9-11h-7z"/>
            </svg>
          </div>
          <h3 class="text-lg font-sans font-semibold text-glass-gray-900 dark:text-dark-glass-gray-900 mb-3">Execution Plan</h3>
          <p class="text-glass-gray-600 dark:text-dark-glass-gray-600 font-sans">Multi-channel campaign orchestration with attribution tracking and optimization loops</p>
        </div>
      </div>

      <!-- Detailed Strategy Timeline -->
      <div class="bg-glass-white/60 dark:bg-dark-glass-white/60 backdrop-blur-lg rounded-glass p-8 border border-glass-gray-200/20 dark:border-dark-glass-gray-200/20 shadow-soft hover:shadow-glass transition-all duration-300">
        <h3 class="text-xl font-serif italic font-semibold text-glass-gray-900 dark:text-dark-glass-gray-900 mb-6 text-center">Implementation Timeline</h3>

        <div class="relative">
          <!-- Timeline Line -->
          <div class="absolute left-4 top-0 bottom-0 w-0.5 bg-glass-gray-300 dark:bg-dark-glass-gray-300"></div>

          <!-- Timeline Items -->
          <div class="space-y-8">
            <!-- Phase 1 -->
            <div class="relative flex items-start space-x-4">
              <div class="w-8 h-8 bg-glass-primary dark:bg-dark-glass-primary rounded-full flex items-center justify-center text-white dark:text-black text-sm font-bold relative z-10">1</div>
              <div class="flex-1 min-w-0">
                <div class="flex items-center space-x-3 mb-2">
                  <h4 class="text-lg font-sans font-semibold text-glass-gray-900 dark:text-dark-glass-gray-900">Research & Positioning</h4>
                  <span class="text-sm text-glass-gray-500 dark:text-dark-glass-gray-500">Weeks 1-3</span>
                </div>
                <p class="text-glass-gray-600 dark:text-dark-glass-gray-600 mb-3">Comprehensive market analysis, competitor mapping, and customer interview program to identify unique positioning opportunities.</p>
                <div class="flex flex-wrap gap-2">
                  <span class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium bg-blue-100 dark:bg-blue-900/30 text-blue-800 dark:text-blue-200">Market Research</span>
                  <span class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium bg-blue-100 dark:bg-blue-900/30 text-blue-800 dark:text-blue-200">Customer Interviews</span>
                  <span class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium bg-blue-100 dark:bg-blue-900/30 text-blue-800 dark:text-blue-200">Competitive Analysis</span>
                </div>
              </div>
            </div>

            <!-- Phase 2 -->
            <div class="relative flex items-start space-x-4">
              <div class="w-8 h-8 bg-glass-primary dark:bg-dark-glass-primary rounded-full flex items-center justify-center text-white dark:text-black text-sm font-bold relative z-10">2</div>
              <div class="flex-1 min-w-0">
